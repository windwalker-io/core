{
    "name": "windwalker/core",
    "description": "Next generation PHP framework.",
    "homepage": "https://github.com/windwalker-io/core",
    "license": "MIT",
    "minimum-stability": "beta",
    "require": {
        "php": ">=8.0.0",
        "ext-json": "*",
        "fakerphp/faker": "^1.0",
        "fig/link-util": "^1.2",
        "filp/whoops": "^2.1",
        "jenssegers/agent": "^2.6",
        "monolog/monolog": "^2.0",
        "nikic/fast-route": "^1.0",
        "nikic/php-parser": "^4.10",
        "relay/relay": "^2.0",
        "asika/symfony-console-completion": "^0.12.0",
        "symfony/console": "^5.2||^6.0",
        "symfony/dotenv": "^5.0||^6.0",
        "symfony/event-dispatcher-contracts": "^2.4",
        "symfony/options-resolver": "^5.0||^6.0",
        "symfony/polyfill-php81": "^1.0",
        "symfony/process": "^5.2||^6.0",
        "symfony/stopwatch": "^5.0||^6.0",
        "webmozart/glob": "^4.0",
        "windwalker/attributes": "^4.0",
        "windwalker/data": "^4.0",
        "windwalker/di": "^4.0",
        "windwalker/environment": "^4.0",
        "windwalker/event": "^4.0",
        "windwalker/filesystem": "^4.0",
        "windwalker/filter": "^4.0",
        "windwalker/http": "^4.0",
        "windwalker/test": "^4.0"
    },
    "require-dev" : {
    },
    "suggest": {
        "dragonmantank/cron-expression": "Install ^3.1 to support schedule runner.",
        "symfony/mailer": "Install ^5.2 to send mail.",
        "tijsverkoyen/css-to-inline-styles": "Install ~2.0 if you want to use mail css inliner."
    },
    "archive": {
        "exclude": [
            ".github",
            "assets",
            "test"
        ]
    },
    "autoload" : {
        "psr-4" : {
            "Windwalker\\" : "src/",
            "Windwalker\\Core\\Test\\": "test/"
        },
        "files": [
            "src/bootstrap.php"
        ]
    },
    "extra": {
        "windwalker": {
            "packages": [
                "Windwalker\\Debugger\\DebuggerPackage"
            ]
        },
        "branch-alias": {
            "dev-master": "4.x-dev"
        }
    }
}
